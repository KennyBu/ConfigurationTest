@model ConfigurationTest.Models.HomeViewModel
@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                The Value from the Configuration is: @Model.Url
                
                <a href="@Model.Url">Click Me</a>
            </p>
        </div>
    </section>
}